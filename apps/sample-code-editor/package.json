{
  "name": "@exec-md/sample-code-editor",
  "version": "1.0.0",
  "description": "Split screen code editor allowing code changes to immediately show",
  "private": true,
  "scripts": {
    "build": "pnpm test && pnpm package",
    "package": "rollup -c 2>&1",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "echo Success",
    "test-watch": "pnpm test -- --watch"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/svelte": "^3.1.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "babel-jest": "^27.5.1",
    "jest": "^27.5.1",
    "rollup": "^2.70.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.6",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.4",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@exec-md/core": "^1.1.0",
    "@exec-md/runtime": "^1.1.0",
    "highlight.js": "^11.5.0",

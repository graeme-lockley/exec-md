{
  "name": "@exec-md/sample-static-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm test && pnpm package",
    "package": "bin/build-notebooks-directory.mjs && rollup -c 2>&1",
    "dev": "bin/build-notebooks-directory.mjs && rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "jest --coverage --coverageReporters='text-summary' 2>&1",
    "test-watch": "pnpm test -- --watch"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.13",
    "babel-jest": "^27.4.6",
    "jest": "^27.4.7",
    "rollup": "^2.66.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.46.3",
    "svelte-check": "^2.3.0",
    "svelte-jester": "^2.1.5",
    "svelte-preprocess": "^4.10.2",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "core-js": "~3.20.3",
    "regenerator-runtime": "~0.13.9"
  },
  "dependencies": {
    "@exec-md/core": "^1.1.0",
    "@exec-md/runtime": "^1.1.0",
    "highlight.js": "^11.4.0",
    "sirv-cli": "^2.0.2"
  }
}

    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "babel-jest": "^27.4.5",
    "jest": "^27.4.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
